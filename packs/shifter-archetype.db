{"name":"Advanced Shapeshifting","type":"feat","effects":[],"system":{"description":{"gm":"","value":"<p>You gain one shifter feat. For the purpose of meeting its prerequisites, your shifter level is equal to half your character level.</p>"},"source":{"value":"Shifter Class"},"rules":[{"key":"ActiveEffectLike","mode":"add","path":"system.custom.modifiers.shifter-dedication-count","value":1},{"adjustName":false,"allowedDrops":{"label":"Shifter feat with level not higher than half your character level","predicate":["item:type:feat","item:trait:hb_mesmerist"]},"choices":{"query":"{\"system.level.value\":{\"$lte\":system.details.level},\"system.traits.value\":{\"$elemMatch\":\"hb_shifter\"}}"},"flag":"advancedShifter","key":"ChoiceSet","prompt":"Choose mesmerist feat with level not higher than half your character level"},{"key":"GrantItem","allowDuplicate":false,"uuid":"{item|flags.pf2e.rulesSelections.advancedShifter}"}],"slug":"advanced-shapeshifting","schema":{"version":0.84,"lastMigration":null},"level":{"value":6},"traits":{"value":["archetype"],"rarity":"common"},"category":"class","onlyLevel1":false,"maxTakable":null,"actionType":{"value":"passive"},"actions":{"value":null},"prerequisites":{"value":[{"value":"Basic Shapeshifting"}]},"location":null},"img":"systems/pf2e/icons/default-icons/feat.svg","flags":{},"_stats":{"systemId":"pf2e","systemVersion":"4.12.9","coreVersion":"10.291","createdTime":1686178325571,"modifiedTime":1686179256706,"lastModifiedBy":"Gh9U9I93FE4tnAIO"},"folder":null,"sort":0,"ownership":{"default":0,"Gh9U9I93FE4tnAIO":3},"_id":"tZSaXTbcMmH1r3Ib"}
